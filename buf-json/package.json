{
  "name": "codeuchain-json-protobuf",
  "version": "1.0.0",
  "description": "Hybrid JSON-Protobuf system for CodeUChain configuration management",
  "main": "json-reader.js",
  "scripts": {
    "test": "node test-comprehensive.js",
    "test:unit": "node test-bidirectional-writer.js",
    "test:schema": "node test-schema-generator.js",
    "test:compiler": "node test-protobuf-compiler.js",
    "test:loader": "node test-protobuf-runtime-loader.js",
    "test:reader": "node test-hybrid-reader.js",
    "test:formatting": "node test-json-formatting.js",
    "benchmark": "node performance-benchmark.js quick",
    "benchmark:comprehensive": "node performance-benchmark.js comprehensive",
    "benchmark:report": "node performance-benchmark.js report",
    "build": "node build.js build",
    "build:watch": "node build.js watch",
    "build:clean": "node build.js clean",
    "build:validate": "node build.js validate",
    "compile-proto": "pbjs -t static-module -w commonjs -o proto-compiled.js config.proto",
    "generate-proto": "pbjs -t json config.proto > config.proto.json",
    "prepare": "npm run build"
  },
  "dependencies": {
    "protobufjs": "^7.5.4"
  },
  "devDependencies": {
    "protobufjs-cli": "^1.1.3",
    "chokidar": "^3.5.3"
  },
  "keywords": [
    "json",
    "protobuf",
    "configuration",
    "sharing",
    "codeuchain",
    "build",
    "hybrid"
  ],
  "author": "CodeUChain",
  "license": "MIT"
}