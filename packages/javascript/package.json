{
  "name": "codeuchain",
  "version": "1.1.1",
  "description": "CodeUChain JavaScript implementation - Universal chain processing with ES5 Legacy and ES6+ support for maximum compatibility",
  "main": "index.auto.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "es5": "./index.es5.js",
      "es6": "./index.es6.js",
      "default": "./index.auto.js"
    },
    "./es5": "./index.es5.js",
    "./es6": "./index.es6.js",
    "./auto": "./index.auto.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "tsc --noEmit",
    "build:es5": "babel core --out-dir dist/es5/core --copy-files",
    "build:es6": "cp -r core dist/es6/",
    "build:all": "npm run build:es5 && npm run build:es6",
    "example": "node examples/simple_chain.js",
    "lint": "eslint core/**/*.js examples/**/*.js",
    "format": "prettier --write core/**/*.js examples/**/*.js"
  },
  "keywords": [
    "codeuchain",
    "chain",
    "context",
    "middleware",
    "functional",
    "async",
    "javascript",
    "typescript",
    "types"
  ],
  "author": "Joshua Wink",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codeuchain/codeuchain.git",
    "directory": "packages/javascript"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "core/",
    "dist/",
    "index.d.ts",
    "types.d.ts",
    "index.es5.js",
    "index.es6.js", 
    "index.auto.js",
    "tsconfig.json",
    "ES5_SUPPORT.md",
    "README.md"
  ],
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-async-to-generator": "^7.22.5",
    "@babel/plugin-transform-runtime": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@types/jest": "^30.0.0",
    "core-js": "^3.32.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "prettier": "^2.0.0",
    "ts-jest": "^29.4.1",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.23.0"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/orchestrate-solutions"
  }
}
